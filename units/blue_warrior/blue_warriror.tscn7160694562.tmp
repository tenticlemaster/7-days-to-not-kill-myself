[sub_resource type="GDScript" id="GDScript_75q1q"]
script/source = "extends State


@export var wander_range: float
@export var speed: float
@onready var character_body: CharacterBody2D = $\"../..\"
@onready var animation_player: AnimationPlayer = $\"../../AnimationPlayer\"
@onready var navigation_agent: NavigationAgent2D = $\"../../NavigationAgent2D\"
var home_position: Vector2
var target_position: Vector2


func _ready() -> void:
	home_position = character_body.global_position


func Enter() -> void:
	animation_player.play(\"run\")
	
	target_position.x = home_position.x + randf_range(-50.0, 50.0)
	target_position.y = home_position.y + randf_range(-50.0, 50.0)
	navigation_agent.target_position = target_position


func PhysicsUpdate(_delta) -> void:
	var direction = character_body.to_local(navigation_agent.get_next_path_position()).normalized()
	
	character_body.velocity = direction * speed
	character_body.move_and_slide()
"

